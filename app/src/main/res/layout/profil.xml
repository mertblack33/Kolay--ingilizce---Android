<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Spinner
        android:id="@+id/gider2"
        android:layout_width="360dp"
        android:layout_height="69dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/arayuz"
        android:clickable="false"
        android:contextClickable="false"
        android:ems="20"
        android:inputType="textPersonName"
        android:scrollbarSize="20dp"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="@+id/textView17"
        app:layout_constraintHorizontal_bias="0.515"
        app:layout_constraintStart_toStartOf="@+id/textView17"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <EditText
        android:id="@+id/isim2"
        android:layout_width="360dp"
        android:layout_height="69dp"
        android:background="@drawable/arayuz"
        android:drawableLeft="@drawable/baseline_person_24"
        android:drawablePadding="20dp"
        android:ems="10"
        android:fontFamily="@font/aclonica"
        android:inputType="textPersonName"
        android:paddingLeft="15dp"
        android:textAlignment="textStart"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintHorizontal_bias="0.552"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        tools:text="MERT" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="283dp"
        android:layout_height="41dp"
        android:background="#D76A6A"
        android:fontFamily="@font/aclonica"
        android:text="KULLANCI ADI"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="283dp"
        android:layout_height="33dp"
        android:layout_marginTop="1dp"
        android:background="#D76A6A"
        android:fontFamily="@font/aclonica"
        android:text="KULLANCI İD"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintHorizontal_bias="0.454"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/isim2" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="283dp"
        android:layout_height="33dp"
        android:background="#D76A6A"
        android:fontFamily="@font/aclonica"
        android:text="SEVİYE SEÇİNİZ"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/textView16"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="@+id/textView16"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="296dp"
        android:layout_height="86dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/aclonica"
        android:text="PROFİL"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="60sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="353dp"
        android:layout_height="62dp"
        android:background="#D76A6A"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        app:srcCompat="@drawable/teamwork" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="360dp"
        android:layout_height="69dp"
        android:background="@drawable/arayuz"
        android:fontFamily="@font/aclonica"
        android:paddingLeft="7dp"
        android:paddingTop="20dp"
        android:paddingRight="7dp"
        android:text="566565"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="19sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/textView15"
        app:layout_constraintHorizontal_bias="0.517"
        app:layout_constraintStart_toStartOf="@+id/textView15"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <Button
        android:id="@+id/button9"
        android:layout_width="128dp"
        android:layout_height="44dp"
        android:background="@drawable/arayuz"
        android:fontFamily="@font/aclonica"
        android:text="KAYDET"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/gider2"
        app:layout_constraintTop_toBottomOf="@+id/gider2" />

    <Button
        android:id="@+id/button27"
        android:layout_width="128dp"
        android:layout_height="44dp"
        android:background="@drawable/arayuz"
        android:fontFamily="@font/aclonica"
        android:text="SINIF SEÇE DÖN"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/gider2"
        app:layout_constraintTop_toBottomOf="@+id/gider2" />
</androidx.constraintlayout.widget.ConstraintLayout>